{
    "description": "Test AES cipher in GCM and CBC modes",
    "transfer_files": [
        "bin/DEMO.8xp"
    ],
    "target": {
        "name": "DEMO",
        "isASM": true
    },
    "sequence": [
        "action|launch",
        "hashWait|1",
        "key|enter",
        "hashWait|2",
        "key|enter",
        "hashWait|3",
        "key|enter",
        "hashWait|4",
        "key|enter"
    ],
    "hashes": {
        "1": {
            "description": "AES-GCM encrypt - GCM Spec (McGrew/Viega), Appendix B, Test Case 4",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "D1F2869F"
            ],
            "timeout": 1500
        },
        "2": {
            "description": "AES-GCM decrypt - GCM Spec (McGrew/Viega), Appendix B, Test Case 4",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "D1F2869F"
            ],
            "timeout": 1500
        },
        "3": {
            "description": "AES-CBC encrypt - NIST SP 800-38A, Appendix F.2.1, CBC-AES128.Encrypt",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "D1F2869F"
            ],
            "timeout": 1500
        },
        "4": {
            "description": "AES-CBC decrypt - NIST SP 800-38A, Appendix F.2.1, CBC-AES128.Decrypt",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "D1F2869F"
            ],
            "timeout": 1500
        }
    }
}