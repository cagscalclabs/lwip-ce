{
    "description": "Test TLS_HASH primitive",
    "transfer_files": [
        "bin/DEMO.8xp"
    ],
    "target": {
        "name": "DEMO",
        "isASM": true
    },
    "sequence": [
        "action|launch",
        "hashWait|1",
        "key|enter",
        "hashWait|2",
        "key|enter",
        "hashWait|3",
        "key|enter"
    ],
    "hashes": {
        "1": {
            "description": "SHA-256 short message 'abc' - NIST FIPS 180-4, Example 1",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "D1F2869F"
            ],
            "timeout": 1000
        },
        "2": {
            "description": "SHA-256 empty message - NIST FIPS 180-4, Empty String",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "D1F2869F"
            ],
            "timeout": 1000
        },
        "3": {
            "description": "SHA-256 two-block message - NIST FIPS 180-4, Example 2",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "D1F2869F"
            ],
            "timeout": 1000
        }
    }
}