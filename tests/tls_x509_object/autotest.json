{
    "description": "Test x.509 TLS certificate decode logic",
    "transfer_files": [
        "bin/DEMO.8xp"
    ],
    "target": {
        "name": "DEMO",
        "isASM": true
    },
    "sequence": [
        "action|launch",
        "hashWait|1",
        "key|enter",
        "hashWait|2",
        "key|enter"
    ],
    "hashes": {
        "1": {
            "description": "Test decode of x.509 certificate with RSA SPKI",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "29DDF567"
            ],
            "timeout": 2000
        },
        "2": {
            "description": "Test decode of x.509 certificate with EC SPKI",
            "start": "vram_start",
            "size": "vram_16_size",
            "expected_CRCs": [
                "E2D4BEBD"
            ],
            "timeout": 2000
        }
    }
}